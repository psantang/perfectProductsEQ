<?xml version='1.0' encoding='utf-8'?>
<widget id="com.paulsantangelo.perfectProductsEQ" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>perfectProductsEQ</name>
    <description>
        Perfect Products Eq.
    </description>
    <author email="paul@paulsantangelo.com" href="http://www.paulsantangelo.com">
        Paul Santangelo Software, Inc.
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <icon src="www/images/pp_seal_125x125.png" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="DisallowOverscroll" value="true" />
