<?xml version='1.0' encoding='utf-8'?>
<widget id="com.paulsantangelo.perfectProductsEQ" version="0.0.01" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <access origin="*" />
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>perfectProductsEQ</name>
    <description>
        Perfect Products Eq.
    </description>
    <author email="paul@paulsantangelo.com" href="http://www.paulsantangelo.com">
        Paul Santangelo Software, Inc.
    </author>
    <content src="index.html" />
    <icon src="www/images/pp_seal_125x125.png" />
    <access launch-external="yes" origin="tel:*" />
    <access launch-external="yes" origin="geo:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
